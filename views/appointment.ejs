<h3><%= viewTitle %></h3>

<form action="/appointment" method="POST" autocomplete="off">
  <div class="form group">
    <label>Date</label>
    <input type="text" class="form-control" name="date" placeholder="dd/mm/yyyy" v">
    <div class="text-danger">
      
    </div>
  </div>
  <div class="form group">
    <label>Name</label>
